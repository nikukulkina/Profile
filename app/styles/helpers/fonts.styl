@font-face
	font-family 'PTSans'
	src url('../fonts/pt_sans.eot')
	src url('../fonts/pt_sans.eot?iefix') format('eot'),
		url('../fonts/pt_sans.woff') format('woff'),
		url('../fonts/pt_sans.ttf') format('truetype'),
		url('../fonts/pt_sans.svg#PTSans-Regular') format('svg')
	font-weight normal
	font-style normal

@font-face
	font-family 'Courier new'
    src url('../fonts/courier_new.eot')
	src url('../fonts/courier_new.eot?iefix') format('eot'),
		url('../fonts/courier_new.woff') format('woff'),
		url('../fonts/courier_new.ttf') format('truetype'),
		url('../fonts/courier_new.svg#PTSans-Regular') format('svg')
	font-weight normal
	font-style normal